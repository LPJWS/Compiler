float sqrt_num = 0.0; float sqrt_ret = 0.0;
float sqrt_eps = 0.001; int sqrt_iter = 0;
def sqrt() {
    sqrt_ret = sqrt_num / 2;
    sqrt_iter = 0;
    while( sqrt_ret - sqrt_num / sqrt_ret > sqrt_eps ){
        sqrt_iter = sqrt_iter + 1;
        sqrt_ret = 0.5 * (sqrt_ret + sqrt_num / sqrt_ret);
    }
}


def main() {
    int num = 64;
    float res = 0.0;
    sqrt_num = num * 1.0; sqrt(); res = sqrt_ret;
    print("root = " + res);
}

main();